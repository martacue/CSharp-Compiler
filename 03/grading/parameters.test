namespace Programa
{
    class A
    {
        public int print_suma (int a , int b, int c) 
        {
            int suma = a + b + c;
            Console.writeLine(suma);
            return suma;
        }
    }

    class Persona
    {
        public string print_info (string nombre, int edad, string ciudad)
        {
            Console.writeLine(nombre);
            Console.writeLine(edad);
            Console.writeLine(ciudad);
            return nombre;
        }
    }
    class Main 
    { 
        public static void main () { 
        A a = new A();
        a.print_suma(5, 3, 2);
        Persona p = new Persona();
        p.print_info("Juan", 20, "Valencia");
        } 
    }

}